{
  "executionRoleArn": "arn:aws:iam::720146896923:role/okura-ecs-role-for-task",
  "containerDefinitions": [
    {
      "name": "fizzbuzz",
      "image": "<IMAGE1_NAME>",
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512",
  "family": "okura-task"
}